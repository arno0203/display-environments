{
  "manifest_version": 2,
  "name": "Display environments",
  "description": "Permet d'identifier l'environnement sur lequel on travail",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "background.html"
  },
  "icons": {
    "128": "img/icon_128.png"
  },
  "content_scripts": [ {
    "matches" : [ "http://*/*", "https://*/*"],
    "js": ["js/jquery-2.2.3.min.js", "js/background.js"]
  } ],
  "background": {
    "scripts": ["js/jquery-2.2.3.min.js", "js/background.js"]
  }
}