{
  "manifest_version": 2,

  "name": "Display environments",
  "description": "Permet d'identifier l'environnement sur lequel on travail",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/icon_128.png"
  }
}